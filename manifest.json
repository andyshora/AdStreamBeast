{
    "manifest_version": 2,
    "name": "AdStreamBeast",
    "version": "1.0",
    "description": "Removes annoying popups on sports streaming sites!",
    "browser_action": { "default_icon": "icon.png" },
    "permissions": [ "http://www.vipboxsports.eu/" ],
    "content_scripts": [ {
        "all_frames": true,
        "js": [ "jquery.min.js", "content.js" ],
        "matches": [ "http://*/*", "https://*/*" ] 
	} ]
}


